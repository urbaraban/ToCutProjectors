<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="controllers\dll\" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="MonchaNETDll">
      <HintPath>dll\MonchaNETDll.DLL</HintPath>
    </Reference>
    <Reference Include="StclLibrary">
      <HintPath>dll\StclLibrary.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Update="controllers\dll\MonchaNETDll.DLL">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
